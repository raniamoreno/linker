{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/notion",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/notion/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}